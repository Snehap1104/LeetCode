class Solution:
    def transpose(self, matrix: List[List[int]]) -> List[List[int]]:
        r = len(matrix)
        c = len(matrix[0])
        a = []
        for i in range(c):
            p=[]
            for  j in range(r):
                p.append(matrix[j][i])
            a.append(p)
        return a        


        
